{
    "manifest_version":3,
    "name":"Notes",
    "description":"Simple chrome extension to take notes while browsing the web",
    "version":"1.0",
    "icons":{
        "128":"img/128.png",
        "48":"img/48.png",
        "32":"img/32.png"
    },
    "action":{
        "icon":"img/128.png",
        "default_popup":"popup.html"
    },

    "permissions":[ "storage", "notifications", "contextMenus" ],

    "background":{
        "service_worker":"eventPage.js"
    }

}